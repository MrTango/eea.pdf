<configure
    xmlns="http://namespaces.zope.org/zope"
    xmlns:browser="http://namespaces.zope.org/browser"
    i18n_domain="eea">

  <browser:page
    for="collective.sendaspdf.tool.ISendAsPDFTool"
    layer="eea.pdf.interfaces.ILayer"
    name="themes"
    template="zpt/themes.pt"
    class=".app.themes.Themes"
    permission="zope2.View"
    />

  <browser:resource
    name="eea.pdf.view.css"
    file="./css/view.css"
   />

  <browser:resource
    name="eea.pdf.view.js"
    file="./js/view.js"
   />

  <browser:resourceDirectory
    name="eea.pdf.images"
    directory="./img" />

</configure>
